{% extends 'blitz/_utilities/diagnostics/layouts/base' %}

{% set crumbs = [
    {
        label: 'Tracked Query String Params',
        path: 'params',
    },
    {
        label: param,
        path: (isLeaf ?? false) ? null : 'params/param?param=' ~ param,
        class: (isLeaf ?? false) ? 'leaf code' : 'code',
    },
]|merge(crumbs ?? []) %}

