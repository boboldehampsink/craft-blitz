{% if page is null %}
    {% redirect 'utilities/blitz-diagnostics' %}
{% endif %}

<div class="blitz-diagnostics">
    {{ include('blitz/_utilities/diagnostics/includes/crumbs', {
        thirdLevel: className(elementType),
    }) }}

    {{ sprig('blitz/_utilities/diagnostics/components/elements', {
        id: page.id,
        elementType: className(elementType),
        displayName: elementType.displayName,
        pluralDisplayName: elementType.pluralDisplayName,
        hasUris: elementType.hasUris,
    }) }}
</div>
